{
  "name": "resin-release",
  "version": "1.2.0",
  "description": "Create releases on Resin without the boilerplate",
  "main": "src/index",
  "types": "src/index.ts",
  "scripts": {
    "clean": "rimraf '{src,test,typings}/**/*.{js,js.map}'",
    "lint": "tslint '{src,test,typings}/**/*.ts'",
    "build": "npm run clean && tsc --project ./",
    "test": "npm run lint && ts-mocha --project ./ test/**/*.spec.ts",
    "prepublish": "npm run build && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/resin-io-modules/resin-release.git"
  },
  "author": "Resin.io",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/resin-io-modules/resin-release/issues"
  },
  "homepage": "https://github.com/resin-io-modules/resin-release#readme",
  "dependencies": {
    "@types/bluebird": "^3.5.18",
    "@types/lodash": "^4.14.87",
    "@types/node": "^8.0.55",
    "@types/request": "^2.0.8",
    "bluebird": "^3.5.1",
    "lodash": "^4.17.4",
    "pinejs-client": "^4.2.3",
    "resin-compose-parse": "^1.0.0",
    "typed-error": "^2.0.0"
  },
  "devDependencies": {
    "@types/chai": "^4.0.8",
    "@types/mocha": "^2.2.44",
    "chai": "^4.1.2",
    "mocha": "^4.0.1",
    "rimraf": "^2.6.2",
    "ts-mocha": "^1.0.3",
    "tslint": "^5.8.0",
    "typescript": "^2.6.2"
  }
}